<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.LauncherController">
   <children>
      <FlowPane columnHalignment="CENTER" maxWidth="300.0" prefHeight="226.0" prefWidth="300.0">
         <children>
             <Text text="Select the program to launch it">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin></Text>
            <Button onAction="#caGlobalProgramBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="C/A Global Generator">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
             <Button onAction="#caProgramBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="C/A Generator">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
             <Button onAction="#correlationBtn" mnemonicParsing="false" text="Correlation">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
             <Button onAction="#gaProgramBtn" mnemonicParsing="false" text="G.A Generator">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
             <Button onAction="#lfsrFastCorrelationBtn" mnemonicParsing="false" text="LFSR Fast Correlation">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
             <Button onAction="#lfsrGeneratorProgramBtn" mnemonicParsing="false" text="LFSR Generator">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
             <Button onAction="#lfsrStatePathProgram" mnemonicParsing="false" text="LFSR State Path">
                 <FlowPane.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                 </FlowPane.margin>
             </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </FlowPane>
   </children>
</Pane>
